//Problem 2
#include <iostream>
#include <conio.h>
#include <iomanip>

using namespace std;

int main()
{
	double mass, density;

		cout << fixed << showpoint << setprecision(2);

		cout << "Please enter the mass or volume in grams: " << endl;
		cin >> mass;
		cout << "Please enter the density in g/cm^3: " << endl;
		cin >> density;
		cout << "The volume is: " << (0.25 * mass)/density << " g/cm^3" << endl;

		_getch();
		return 0;
}
